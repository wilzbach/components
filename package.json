{
  "name": "asyncy-lib",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve ./showcase/main.js",
    "build:lib": "vue-cli-service build --target lib --name asyncy-vue ./src/lib.js",
    "copy:files": "cpx ./src/styles/ ./dist",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -c .storybook -o dist_storybook",
    "build": "yarn build:lib && yarn build:storybook && yarn copy:files"
  },
  "dependencies": {
    "asyncy-player": "^0.0.1",
    "bootstrap": "^4.1.3",
    "nouislider": "^12.0.0",
    "popper.js": "^1.14.4",
    "vue": "^2.5.17",
    "vue-clipboard2": "^0.2.1",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.1",
    "vue2-transitions": "^0.2.3"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addon-notes": "^3.4.11",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/vue": "^3.4.11",
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/eslint-config-standard": "^3.0.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-preset-vue": "^2.0.2",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "storybook-addon-jsx": "^5.4.0",
    "storybook-vue-router": "^1.0.1",
    "vue-template-compiler": "^2.5.17"
  }
}
